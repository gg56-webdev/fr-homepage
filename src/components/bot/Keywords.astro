---
import {t} from 'i18next'
---
<section class="mx-auto px-2 relative z-10 flex-wrap gap-6 text-center  max-w-screen-xl flex justify-evenly ">
  {(t('bot.keywords', {returnObjects: true})as string[]).map(w => <div class='p-6 rounded-xl bg-slate-950/80'>
    <h2 class="text-2xl md:text-3xl brand-text font-bold">{w}</h2>
  </div>)}
</section>